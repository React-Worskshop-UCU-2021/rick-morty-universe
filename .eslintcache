;\n    const [status, setStatus] = useState('');\n\n    const delayedQuery = useCallback(_.debounce(setQueryName, 1000), [\n        setQueryName,\n    ]);\n\n    const onNameChange = (value) => {\n        setName(value);\n        delayedQuery(value);\n    };\n\n    return (\n        <div className=\"Home\">\n            <Hero\n                characterName={name}\n                setCharacterName={onNameChange}\n                gender={gender}\n                setGender={setGender}\n                status={status}\n                setStatus={setStatus}\n            />\n            <UserList gender={gender} status={status} name={queryName} />\n        </div>\n    );\n}\n\nexport default Home;\n","/Users/olehmykytyn/rick-morty-universe/src/components/Header/Header.js",[],"/Users/olehmykytyn/rick-morty-universe/src/pages/DetailedCharacter/DetailedCharacter.js",["139"],"import { useEffect, useState } from 'react';\nimport { useParams, Link } from 'react-router-dom';\n\nimport { getCharacter } from '../../api';\nimport Tag from '../../components/Tag';\nimport LabelValueSet from '../../components/LabelValueSet';\nimport Label from '../../components/Label';\nimport Value from '../../components/Value';\n\nimport './DetailedCharacter.scss';\n\nconst DetailedCharacter = () => {\n    const { id } = useParams();\n\n    const [character, setCharacter] = useState();\n\n    useEffect(() => {\n        loadCharacter(id);\n    }, [id]);\n\n    const loadCharacter = async (id) => {\n        const item = await getCharacter(id);\n        if (item.error) {\n            console.log(item.error);\n        } else {\n            setCharacter(item);\n        }\n    };\n\n    const {\n        name,\n        status,\n        gender,\n        species,\n        origin,\n        created,\n        image,\n        episode,\n        location,\n    } = character || {};\n\n    const getEpisodes = episode?.map((episode) => (\n        <Value value={episode} key={episode} />\n    ));\n\n    return character ? (\n        <div className=\"DetailedCharacter\">\n            <div className=\"DetailedCharacter__navigationContainer\">\n                <Link to=\"/\" className=\"DetailedCharacter__homeLink\">\n                    <Label\n                        className=\"DetailedCharacter__homeLabel\"\n                        label=\"Home\"\n                    />\n                </Link>\n                <Label label=\"|\" />\n                <Label label={`#${id} ${name}`} />\n            </div>\n\n            <div className=\"DetailedCharacter__container\">\n                <div className=\"DetailedCharacter__imageContainer\">\n                    <img src={image} className=\"DetailedCharacter__image\" />\n                </div>\n                <div className=\"DetailedCharacter__descriptionContainer\">\n                    <h1 className=\"DetailedCharacter__heading\">\n                        #{id} {name}\n                    </h1>\n                    <div className=\"DetailedCharacter__tagsContainer\">\n                        <Tag children={status} />\n                        <Tag children={gender} />\n                    </div>\n                    <div className=\"DetailedCharacter__infoContainer\">\n                        <div className=\"DetailedCharacter__mainInfo\">\n                            <LabelValueSet label=\"Species: \" value={species} />\n                            <LabelValueSet\n                                label=\"Origin: \"\n                                value={origin.name}\n                            />\n                            <LabelValueSet label=\"Birthday: \" value={created} />\n                            <LabelValueSet\n                                label=\"Last Known location: \"\n                                value={location.name}\n                            />\n                            <LabelValueSet\n                                label=\"First seen in: \"\n                                value={origin.name}\n                            />\n                        </div>\n                        <div className=\"DetailedCharacter__episodesContainer\">\n                            <Label label=\"Episodes: \" />\n                            {getEpisodes}\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    ) : (\n        <div className=\"DetailedCharacter\">Error 404: Not Found</div>\n    );\n};\n\nexport default DetailedCharacter;\n","/Users/olehmykytyn/rick-morty-universe/src/components/UserList/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Hero/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/api/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Tag/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Value/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/LabelValueSet/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Search/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Label/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Tag/Tag.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/UserList/UserList.js",["140"],"import { useState, useEffect } from 'react';\nimport PropTypes from 'prop-types';\n\nimport Card from '../Card';\nimport { getCharacters } from '../../api';\nimport Pagination from '../../components/Pagination';\n\nimport './UserList.scss';\n\nconst UserList = ({ gender, status, name }) => {\n    const [currentPage, setCurrentPage] = useState(0);\n    const [pages, setPages] = useState(0);\n    const [isLoading, setIsLoading] = useState(false);\n\n    const [characters, setCharacters] = useState();\n\n    const loadCharacters = async (page = 0, params) => {\n        setIsLoading(true);\n        const items = await getCharacters(page + 1, params);\n        setIsLoading(false);\n        if (items.error) {\n            console.log(items.error);\n        } else {\n            setCharacters(items?.results);\n            setPages(items?.info?.pages || 0);\n        }\n    };\n\n    useEffect(() => {\n        loadCharacters(currentPage, {\n            ...(name && { name }),\n            ...(gender && { gender }),\n            ...(status && { status }),\n        });\n    }, [name, gender, status, currentPage]);\n\n    useEffect(() => {\n        setCurrentPage(0);\n    }, [name, gender, status]);\n\n    const renderCharacter = (character) => (\n        <Card key={character.id} {...character} />\n    );\n\n    return (\n        <div className=\"UserList\">\n            <div className=\"UserList__cardList\">\n                {characters?.map(renderCharacter)}\n            </div>\n            <Pagination\n                pages={pages}\n                currentPage={currentPage}\n                setCurrentPage={setCurrentPage}\n            />\n        </div>\n    );\n};\n\nUserList.propTypes = {\n    name: PropTypes.string.isRequired,\n    status: PropTypes.oneOf(['Alive', 'Dead', 'unknown', '']).isRequired,\n    gender: PropTypes.oneOf(['Male', 'Female', 'unknown', 'Genderless', ''])\n        .isRequired,\n};\n\nexport default UserList;\n","/Users/olehmykytyn/rick-morty-universe/src/components/Search/Search.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/LabelValueSet/LabelValueSet.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Value/Value.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Hero/Hero.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Label/Label.js",[],"/Users/olehmykytyn/rick-morty-universe/src/utils/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Pagination/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Card/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Select/index.js",[],"/Users/olehmykytyn/rick-morty-universe/src/utils/consts.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Pagination/Pagination.js",["141"],"import { PropTypes } from 'prop-types';\n\nimport _ from 'lodash';\n\nimport './Pagination.scss';\n\nconst Pagination = ({ pages, setCurrentPage, currentPage }) => {\n    const setPagesArray = _(Array(pages))\n        .fill(null)\n        .map((el, ind) => ind)\n        .value();\n\n    const getDisplayedPages = () => {\n        if (currentPage - 2 <= 0) return _.range(0, Math.min(5, pages));\n        if (currentPage + 2 >= pages)\n            return _.range(Math.max(pages - 5, 0), pages);\n        const minPage = Math.max(currentPage - 2, 0);\n        const maxPage = Math.min(minPage + 4, pages);\n        return _.range(minPage, maxPage + 1);\n    };\n\n    const handleClick = (page, isClickable = true) => () => {\n        if (!isClickable) return;\n        setCurrentPage(page);\n    };\n\n    const renderPage = (page) => {\n        const isActive = page === currentPage;\n        return (\n            <div\n                onClick={handleClick(page)}\n                className={`Pagination__page ${\n                    isActive ? 'Pagination__page_active' : ''\n                }`}\n                key={page}\n            >\n                {page + 1}\n            </div>\n        );\n    };\n\n    const renderArrowButton = (isNext = false) => {\n        const nextPage = isNext ? currentPage + 1 : currentPage - 1;\n        const isArrowClickable =\n            (!isNext && nextPage >= 0) || (isNext && nextPage < pages);\n        return (\n            <div\n                className=\"Pagination__page\"\n                onClick={handleClick(nextPage, isArrowClickable)}\n            >\n                <div\n                    className={`Pagination__arrow ${\n                        isNext ? 'Pagination__arrow_next' : ''\n                    } ${isArrowClickable ? 'Pagination__arrow_active' : ''}`}\n                />\n            </div>\n        );\n    };\n\n    return (\n        <div className=\"Pagination\">\n            {renderArrowButton()}\n            {_.map(getDisplayedPages(), renderPage)}\n            {renderArrowButton(true)}\n        </div>\n    );\n};\n\nPagination.propTypes = {\n    pages: PropTypes.number.isRequired,\n    setCurrentPage: PropTypes.func.isRequired,\n    currentPage: PropTypes.number.isRequired,\n};\n\nexport default Pagination;\n","/Users/olehmykytyn/rick-morty-universe/src/components/Select/Select.js",[],"/Users/olehmykytyn/rick-morty-universe/src/components/Card/Card.js",["142"],"import { Link } from 'react-router-dom';\nimport { PropTypes } from 'prop-types';\n\nimport Tag from '../Tag';\nimport LabelValueSet from '../LabelValueSet';\n\nimport './Card.scss';\n\nconst Card = ({ id, name, image, status, gender, location, origin }) => {\n    return (\n        <div className=\"Card\">\n            <div className=\"Card__imageHolder\"> </div>\n            <Link to={`character/${id}`}>\n                <img className=\"Card__image\" src={image} />\n            </Link>\n            <div className=\"Card__content\">\n                <Link className=\"Card__link\" to={`character/${id}`}>\n                    <h4 className=\"Card__name\">{name}</h4>\n                </Link>\n                <div className=\"Card__tags\">\n                    <Tag className=\"Card__tag\" children={status}></Tag>\n                    <Tag className=\"Card__tag\" children={gender}></Tag>\n                </div>\n                <div className=\"DetailedCharacter__mainInfo\">\n                    <LabelValueSet\n                        label=\"Last Known location: \"\n                        value={location.name}\n                    />\n                    <LabelValueSet\n                        label=\"First seen in: \"\n                        value={origin.name}\n                    />\n                </div>\n            </div>\n        </div>\n    );\n};\n\nCard.propTypes = {\n    name: PropTypes.string.isRequired,\n    status: PropTypes.oneOf(['Alive', 'Dead', 'unknown']).isRequired,\n